{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/meet-up/src",
  "compilerOptions": {
    "typeCheck": true,
    "webpack": true,
    "webpackConfigPath": "./webpack.config.js",
    "tsConfigPath": "apps/meet-up/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/meet-up",
  "projects": {
    "members": {
      "type": "application",
      "root": "apps/members",
      "entryFile": "main",
      "sourceRoot": "apps/members/src",
      "compilerOptions": {
        "tsConfigPath": "apps/members/tsconfig.app.json"
      }
    },
    "meetings": {
      "type": "application",
      "root": "apps/meetings",
      "entryFile": "main",
      "sourceRoot": "apps/meetings/src",
      "compilerOptions": {
        "tsConfigPath": "apps/meetings/tsconfig.app.json"
      }
    },
    "attendances": {
      "type": "application",
      "root": "apps/attendances",
      "entryFile": "main",
      "sourceRoot": "apps/attendances/src",
      "compilerOptions": {
        "tsConfigPath": "apps/attendances/tsconfig.app.json"
      }
    },
    "entities": {
      "type": "library",
      "root": "libs/entities",
      "entryFile": "index",
      "sourceRoot": "libs/entities/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entities/tsconfig.lib.json"
      }
    },
    "config": {
      "type": "library",
      "root": "libs/config",
      "entryFile": "index",
      "sourceRoot": "libs/config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/config/tsconfig.lib.json"
      }
    },
    "log": {
      "type": "library",
      "root": "libs/log",
      "entryFile": "index",
      "sourceRoot": "libs/log/src",
      "compilerOptions": {
        "tsConfigPath": "libs/log/tsconfig.lib.json"
      }
    }
  }
}