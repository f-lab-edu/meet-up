{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/meet-up/src",
  "compilerOptions": {
    "typeCheck": true,
    "webpack": true,
    "webpackConfigPath": "./webpack.config.js",
    "tsConfigPath": "apps/meet-up/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/meet-up",
  "projects": {
    "meet-up": {
      "type": "application",
      "root": "apps/meet-up",
      "entryFile": "main",
      "sourceRoot": "apps/meet-up/src",
      "compilerOptions": {
        "tsConfigPath": "apps/meet-up/tsconfig.app.json"
      }
    },
    "members": {
      "type": "application",
      "root": "apps/members",
      "entryFile": "main",
      "sourceRoot": "apps/members/src",
      "compilerOptions": {
        "tsConfigPath": "apps/members/tsconfig.app.json"
      }
    },
    "meetings": {
      "type": "application",
      "root": "apps/meetings",
      "entryFile": "main",
      "sourceRoot": "apps/meetings/src",
      "compilerOptions": {
        "tsConfigPath": "apps/meetings/tsconfig.app.json"
      }
    },
    "attendances": {
      "type": "application",
      "root": "apps/attendances",
      "entryFile": "main",
      "sourceRoot": "apps/attendances/src",
      "compilerOptions": {
        "tsConfigPath": "apps/attendances/tsconfig.app.json"
      }
    }
  }
}
